{
    "categories": [
        {
            "title": "## ‚ùó Deprecations",
            "labels": [
                "deprecation"
            ]
        },
        {
            "title": "## üöÄ New Features",
            "labels": [
                "enhancement"
            ]
        },
        {
            "title": "## üêõ Bug Fixes",
            "labels": [
                "bug"
            ]
        },
        {
            "title": "## üìò Documentation",
            "labels": [
                "documentation"
            ]
        },
        {
            "title": "## ‚öôÔ∏è OmniSciDB",
            "labels": [
                "omniscidb"
            ]
        }
    ],
    "ignore_labels": [
        "Skip-Changelog"
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
    "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
    "empty_template": "- no changes",
    "label_extractor": [
        {
            "pattern": "(.) (.+)",
            "target": "$1"
        }
    ],
    "transformers": [
        {
            "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
            "target": "- $4\n  - $6"
        }
    ],
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 365,
    "exclude_merge_branches": [
        "Owner/qa"
    ],
    "tag_resolver": {
        "method": "semver"
    }
}